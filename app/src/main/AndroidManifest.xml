<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.azharul.android.sids">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/three"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".StudentFormActivity"
            android:parentActivityName=".StudentInformationActivity"
            android:label="@string/app_form"/>
        <activity android:name=".StudentActivity"
            android:parentActivityName=".StudentInformationActivity"
            android:label="@string/app_student"/>
        <activity android:name=".StudentInformationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
